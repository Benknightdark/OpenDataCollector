{
    "Routes": [
      {
        "DownstreamPathTemplate": "/v1.0/invoke/{everything}/method/api",
        "DownstreamScheme": "http",
        "DownstreamHostAndPorts": [
          {
            "Host": "localhost",
            "Port": 3500
          }
        ],
        "RouteIsCaseSensitive": false,
        "UpstreamPathTemplate": "/api/{everything}",
        "UpstreamHttpMethod": [
          "Get",
          "Post",
          "Put",
          "Delete"
        ]     
      }
      
    ],
    "GlobalConfiguration": {
      "BaseUrl": "http://localhost:5000"
    }
  }